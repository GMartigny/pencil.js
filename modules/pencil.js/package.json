        "@pencil.js/rotatable": "^1.10.2",
        "@pencil.js/scene": "^1.10.3",
        "@pencil.js/select": "^1.10.2",
        "@pencil.js/slider": "^1.10.3",
        "@pencil.js/spline": "^1.10.2",
        "@pencil.js/square": "^1.10.2",
        "@pencil.js/star": "^1.10.2",
