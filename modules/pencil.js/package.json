{
  "name": "pencil.js",
  "version": "1.0.2",
  "description": "Modular 2D drawing library.",
  "main": "pencil.js",
  "files": [
    "dist/"
  ],
  "unpkg": "dist/pencil.min.js",
  "jsdelivr": "dist/pencil.min.js",
